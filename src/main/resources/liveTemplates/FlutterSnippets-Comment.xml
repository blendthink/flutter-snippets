<templateSet group="FlutterSnippets-Comment">
  <template name="todo" value="// TODO($USER$): $TODO$" description="adds // TODO" toReformat="false" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="TODO" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART_TOPLEVEL" value="true" />
    </context>
  </template>
</templateSet>
